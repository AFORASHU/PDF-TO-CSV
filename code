import tabula 
df = tabula.read_pdf(r'C:\Users\SESA595715\Pictures\Saved Pictures\c.pdf', pages = "all")
tabula.convert_into(r"C:\Users\SESA595715\Pictures\Saved Pictures\c.pdf", r'C:\Users\SESA595715\Pictures\Saved Pictures\c.csv', output_format="csv",pages='all', stream=True)
